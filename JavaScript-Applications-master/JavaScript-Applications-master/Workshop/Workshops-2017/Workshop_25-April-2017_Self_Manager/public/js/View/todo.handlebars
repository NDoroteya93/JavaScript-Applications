    	<div id="todocontainer" class="row">
			<div class="col-md-6 col-md-offset-3">
				<div class="panel panel-primary">
					<div class="panel-heading">
						<h3 class="panel-title">TODO List</h3>
						<div class="pull-right">
							<span class="clickable filter" data-toggle="tooltip" title="Add TODO" data-container="body">
								<i class="glyphicon glyphicon-plus"></i>
							</span>
						</div>
					</div>
					<div class="panel-body">
						  <div class="form-group">
								<label for="email">Text:</label>
								<input type="text" class="form-control" id="text-todo" placeholder="Enter text" name="email">
							</div>
							<div class="form-group">
								<label for="pwd">Category:</label>
								<input id ="category-todo" type="text" class="form-control" placeholder="Enter category" name="pwd">
							</div>
							<div class="checkbox">
								<label><input id="check-state" type="checkbox" name="remember"></label>
							</div>
							<a href="#/todo/add" id="btn-add-todo" class="btn btn-default">Add</a>
					</div>
					<table class="table table-hover" id="dev-table">
						<thead>	
							<tr>
								<th>#</th>
								<th>Text</th>
								<th>Category</th>
								<th>State</th>
							</tr>
						</thead>
						<tbody>
							 {{#each this}}
							<tr>
								<td>{{@index}}</td>

								<td>{{text}}</td>
								{{#if state}}
								<td>
									<div class="checkbox">
										<label><input type="checkbox" checked>State</label>
									</div>
								</td>
								{{else}}
								<td>
									<div class="checkbox">
										<label></label><input type="checkbox">State</label>
									</div>
								</td>
								{{/if}}

								<td>{{category}}</td>
							</tr>
							{{/each}}
						</tbody>
					</table>
				</div>
			</div>
		</div>